@model IEnumerable<Aplicacao.ViewModels.VeiculoViewModel>
<div class="col-xs-12">
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Modelo.Marca.Nome)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Modelo.Descricao)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TipoVeiculo)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Placa)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cor)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ano)
                </th>
                <th class="hasinput" style="max-width: 120px; min-width: 120px; width: 1%;" rowspan="1" colspan="1">Ações</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.Modelo.Marca.Nome)
                        </td>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.Modelo.Descricao)
                        </td>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.TipoVeiculo)
                        </td>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.Placa)
                        </td>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.Cor)
                        </td>
                        <td class="col-xs-10">
                            @Html.DisplayFor(modelItem => item.Ano)
                        </td>
                        <td class="col-md-2">
                            <span class="btn btn-primary" style="margin-right: 10px;"><i class="fa fa-edit"></i></span>
                            <span class="btn btn-danger"><i class="fa fa-remove"></i></span>
                        </td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>