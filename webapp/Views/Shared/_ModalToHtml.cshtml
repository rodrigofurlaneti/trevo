<!-- Modal -->
<div class="modal fade" id="modalToHtml" tabindex="-1" role="dialog" aria-labelledby="modalToHtmlCenterTitle"
     data-toggle="modal" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content" id="modal-content">
            <div class="modal-header panel-heading" style="display: inherit !important;">
                <h5 class="modal-title" id="modalToHtmlTitle">Modal title</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div id="modalToHtmlBody" class="modal-body">
            </div>
            <div class="modal-footer">
                <input type="button" id="btnModalToHtmlAction" class="btn btn-primary" value=""/>
                <input type="button" id="btnModalToHtmlClose" class="btn btn-default" data-dismiss="modal" value="Fechar"/>
            </div>
        </div>
    </div>
</div>