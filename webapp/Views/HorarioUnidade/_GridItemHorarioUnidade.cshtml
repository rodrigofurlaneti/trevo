@model IEnumerable<Aplicacao.ViewModels.UnidadeCheckListAtividadeViewModel>

<div>
    @if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {

            <div id="conteudo" class="row margin-bottom-5">
                <div class="col-xs-2">
                    @Html.DisplayFor(modelItem => item.CheckListAtividade.Descricao)
                </div>
                <div class="col-xs-2">
                     @Html.CheckBoxFor(modelItem => item.Checkado)
                </div>
           
            </div>
        }
    }
</div>
